    <script>
        const products = {
            "uvc-doorbell-b": { name: "Doorbell Lite", model: "UVC-Doorbell-B" },
            "unas-2-b": { name: "UNAS 2", model: "UNAS-2-B" },
            "uvc-g4-doorbell-pro": { name: "G4 Doorbell Pro", model: "UVC-G4-Doorbell-Pro" },
            "uvc-g5-flex": { name: "Camera G5 Flex", model: "UVC-G5-Flex" },
            "uvc-g6-180": { name: "Camera G6 180", model: "UVC-G6-180" },
            "usl-motion": { name: "Smart Sensor Motion", model: "USL-Motion" },
            "uvc-g5-turret-ultra": { name: "Camera G5 Turret Ultra", model: "UVC-G5-Turret-Ultra" },
            "u7-pro-xg": { name: "U7 Pro XG", model: "U7-Pro-XG" },
            "usw-flex-2-5g-8-poe": { name: "Switch Flex 2.5G PoE (8-port)", model: "USW-Flex-2.5G-8-PoE" },
            "ucg-max": { name: "Cloud Gateway Max", model: "UCG-Max" },
            "ucg-fiber": { name: "Cloud Gateway Fiber", model: "UCG-Fiber" },
            "dream-router-7": { name: "Dream Router 7", model: "UDR7" },
            "pbe-5ac-gen2": { name: "PowerBeam 5AC Gen2", model: "PBE-5AC-Gen2" },
            "pbe-5ac-500": { name: "PowerBeam 5AC 500", model: "PBE-5AC-500" }
        };

        let quoteItems = [];

        function addItem() {
            const productId = document.getElementById('product').value;
            const quantity = parseInt(document.getElementById('quantity').value);
            const unitPrice = parseFloat(document.getElementById('unitPrice').value);

            if (!productId) {
                alert('Please select a product');
                return;
            }

            if (!unitPrice || unitPrice < 0) {
                alert('Please enter a valid unit price');
                return;
            }

            const product = products[productId];
            const total = quantity * unitPrice;

            quoteItems.push({
                name: product.name,
                model: product.model,
                quantity: quantity,
                unitPrice: unitPrice,
                total: total
            });

            updateItemsTable();
            updateTotals();

            document.getElementById('product').value = '';
            document.getElementById('quantity').value = '1';
            document.getElementById('unitPrice').value = '';
        }

        function removeItem(index) {
            quoteItems.splice(index, 1);
            updateItemsTable();
            updateTotals();
        }

        function updateItemsTable() {
            const tbody = document.getElementById('itemsBody');
            
            if (quoteItems.length === 0) {
                tbody.innerHTML = `
                    <tr>
                        <td colspan="6" style="text-align: center; color: #9B8170; padding: 30px;">
                            No items added yet. Add products above to build your quote.
                        </td>
                    </tr>
                `;
                return;
            }

            tbody.innerHTML = quoteItems.map((item, index) => `
                <tr>
                    <td>${item.name}</td>
                    <td>${item.model}</td>
                    <td class="text-right">${item.quantity}</td>
                    <td class="text-right">$${item.unitPrice.toFixed(2)}</td>
                    <td class="text-right">$${item.total.toFixed(2)}</td>
                    <td class="no-print">
                        <button class="danger remove-btn" onclick="removeItem(${index})">Remove</button>
                    </td>
                </tr>
            `).join('');
        }

        function updateTotals() {
            const equipmentTotal = quoteItems.reduce((sum, item) => sum + item.total, 0);
            const hourlyRate = parseFloat(document.getElementById('hourlyRate').value) || 0;
            const laborHours = parseFloat(document.getElementById('laborHours').value) || 0;
            const laborTotal = hourlyRate * laborHours;
            const grandTotal = equipmentTotal + laborTotal;

            document.getElementById('equipmentTotal').textContent = `$${equipmentTotal.toFixed(2)}`;
            document.getElementById('laborTotal').textContent = `$${laborTotal.toFixed(2)}`;
            document.getElementById('grandTotal').textContent = `$${grandTotal.toFixed(2)}`;

            document.getElementById('totalsSection').style.display = quoteItems.length > 0 ? 'block' : 'none';
        }

        function generateQuote() {
            const clientName = document.getElementById('clientName').value;
            
            if (!clientName) {
                alert('Please enter a client name');
                return;
            }

            if (quoteItems.length === 0) {
                alert('Please add at least one item to the quote');
                return;
            }

            const now = new Date();
            const quoteNumber = `KN-${now.getFullYear()}${(now.getMonth()+1).toString().padStart(2,'0')}${now.getDate().toString().padStart(2,'0')}-${now.getHours().toString().padStart(2,'0')}${now.getMinutes().toString().padStart(2,'0')}`;
            const quoteDate = now.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' });

            document.getElementById('quoteNumber').textContent = quoteNumber;
            document.getElementById('quoteDate').textContent = quoteDate;
            document.getElementById('previewClient').textContent = clientName;
            document.getElementById('previewProject').textContent = document.getElementById('projectName').value || 'Network Installation';

            const previewBody = document.getElementById('previewItemsBody');
            previewBody.innerHTML = quoteItems.map(item => `
                <tr>
                    <td>${item.name}</td>
                    <td>${item.model}</td>
                    <td class="text-right">${item.quantity}</td>
                    <td class="text-right">$${item.unitPrice.toFixed(2)}</td>
                    <td class="text-right">$${item.total.toFixed(2)}</td>
                </tr>
            `).join('');

            const equipmentTotal = quoteItems.reduce((sum, item) => sum + item.total, 0);
            const hourlyRate = parseFloat(document.getElementById('hourlyRate').value) || 0;
            const laborHours = parseFloat(document.getElementById('laborHours').value) || 0;
            const laborTotal = hourlyRate * laborHours;
            const grandTotal = equipmentTotal + laborTotal;

            document.getElementById('previewEquipmentTotal').textContent = `$${equipmentTotal.toFixed(2)}`;
            document.getElementById('previewLaborTotalAmount').textContent = `$${laborTotal.toFixed(2)}`;
            document.getElementById('previewGrandTotal').textContent = `$${grandTotal.toFixed(2)}`;

            if (laborTotal > 0) {
                document.getElementById('laborSection').style.display = 'block';
                document.getElementById('previewHours').textContent = laborHours.toFixed(1);
                document.getElementById('previewRate').textContent = `$${hourlyRate.toFixed(2)}/hr`;
                document.getElementById('previewLaborTotal').textContent = `$${laborTotal.toFixed(2)}`;
            } else {
                document.getElementById('laborSection').style.display = 'none';
            }

            const monthlyMgmt = parseFloat(document.getElementById('monthlyMgmt').value) || 0;
            if (monthlyMgmt > 0) {
                document.getElementById('monthlySection').style.display = 'block';
                document.getElementById('previewMonthly').textContent = `$${monthlyMgmt.toFixed(2)}/month`;
            } else {
                document.getElementById('monthlySection').style.display = 'none';
            }

            document.querySelector('.form-section').style.display = 'none';
            document.getElementById('quotePreview').style.display = 'block';
            window.scrollTo(0, 0);
        }

        function editQuote() {
            document.querySelector('.form-section').style.display = 'block';
            document.getElementById('quotePreview').style.display = 'none';
        }

        document.getElementById('hourlyRate').addEventListener('input', updateTotals);
        document.getElementById('laborHours').addEventListener('input', updateTotals);
    </script>
</body>
</html>
